{
  "name": "gae-node-datastore",
  "version": "0.0.0",
  "description": "An example repo of Node on Google App Engine Standard with Datastore (plus local emulator)",
  "main": "app.js",
  "repository": "https://github.com/chriscalo/gae-node-datastore.git",
  "author": "Chris Calo <chris.calo@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "serve": "nodemon app.js",
    "dev": "run-s dev:datastore-env serve",
    "dev:datastore": "gcloud beta emulators datastore start --project=foo",
    "dev:datastore-env": "$(gcloud beta emulators datastore env-init)"
  },
  "dependencies": {
    "@google-cloud/datastore": "^2.0.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "opn": "^5.4.0"
  }
}
